{
  "rules": {
    "no-unused-variable": [
      true
    ],
    "no-console": [true, "log", "error"],
    "no-any": true,
    "no-empty-interface": true,
    "member-access": [true, "check-constructor", "check-parameter-property"],
    "no-unnecessary-type-assertion": true,
    "promise-function-async": true,
    "no-conditional-assignment": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-super": true,
    "no-duplicate-variable": [true, "check-parameters"],
    "no-dynamic-delete": true,
    "no-use-before-declare": true,
    "triple-equals": [true, "allow-null-check", "allow-undefined-check"],
    "switch-default": true,
    "prefer-const": true,
    "prefer-readonly": true,
    "array-type": [true, "generic"],
    "no-boolean-literal-compare": true,
    "no-consecutive-blank-lines": true,
    "no-trailing-whitespace": [true, "ignore-comments"],
    "no-unnecessary-qualifier": true,
    "quotemark": [true, "single", "avoid-escape"],
    "semicolon": [true, "always", "ignore-interfaces"],
    "whitespace": [true, "check-decl", "check-branch", "check-operator", "check-typecast", "check-rest-spread"],
    "no-unnecessary-initializer": true,
    "newline-before-return": true,
    "interface-over-type-literal": true,
    "interface-name": [true, "always-prefix"],
    "binary-expression-operand-order": true,
    "arrow-return-shorthand": true,
    "no-empty": true,
    "typedef": [true, "call-signature", "parameter", "member-variable-declaration", "object-destructuring", "array-destructuring"]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
